<%=form_for @user do |f| %>

  <fieldset>
    <legend>Create an Account </legend>
    <div>
      <%= f.label :username %>
      <%= f.text_field :username%>
    </div>
    <div>
      <%=f.label :password %>
      <%= f.password_field :password %>
    </div>

    <div>
      <%= f.label :password_confirmation%>
      <%= f.password_field :password_confirmation%>
    </div>

    <div>
      <%=f.submit 'Submit'%>
    </div>
  </fieldset>
<%end%>