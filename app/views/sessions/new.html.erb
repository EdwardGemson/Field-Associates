<%= form_tag session_path do %>
  <fieldset>
    <legend>login</legend>
    <div>
      <%= label_tag :username%>
      <%= text_field_tag :username %>
    </div>

    <div>
      <%= label_tag :password %>
      <%= text_field_tag :password %>
    </div>
    <div>
      <%= submit_tag 'Log in'%>
    </div>
  </fieldset>
<% end %>